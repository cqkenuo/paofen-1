
.Real_name {
	height: 46rpx;
	border: #687c99 solid 3rpx;
	margin-left: 250rpx;
	border-radius: 25rpx;
	line-height: 46rpx;
	color: #687c99;
	padding: 0 15rpx;
	font-size: 28rpx;
}
.active {
	border: #7ce2f5 solid 3rpx;
	color: #7ce2f5;
}
.titlenav {
	width: 94%;
	background: #fff;
	border-radius: 30rpx;
	margin: 20rpx auto; 
	position: relative;
	margin: 3%;
	-webkit-box-shadow: 0px 3px 10px #888888;
	        box-shadow: 0px 3px 10px #888888;
}
.float {
	width: 40%;
	margin: 13rpx;
	padding: 20rpx;
	position: relative;
}
.textnav {
	color: #687c99;
	font-size: 30rpx;
	padding-left: 20rpx;
}
.textnav3 {
	color: #687c99;
	font-size: 28rpx;
	margin-top: 80rpx;
	text-align: center;
}
.username {
	width: 372rpx;
	font-size: 38rpx;
	font-family: PingFang SC;
	font-weight: bold;
	color: rgba(1, 10, 17, 1);
	/* padding-top: 30upx; */
}
.listnav {
	margin: auto;
	width: 88%;
	height: 40rpx;
	border-top: rgba(223, 223, 223, 1) solid 0.5rpx;
	line-height: 40rpx;
	border-left: none;
	border-right: none;
	padding: 20rpx;
}
.icon {
	width: 65rpx;
	height: 60rpx;
	position: absolute;
	left: 50%;
	margin-left: -32rpx;
}
.login-btn {
	padding: 10rpx 20rpx;
	margin-top: 50rpx;
}
.landing {
	width: 50%;
	height: 84rpx;
	line-height: 84rpx;
	border-radius: 10rpx;
	font-size: 32rpx;
	background: -webkit-gradient(linear, left top, right top, from(#4286de), to(#5091e6));
	background: -o-linear-gradient(left, #4286de, #5091e6);
	background: linear-gradient(left, #4286de, #5091e6);
}
.phone_wrap {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	height: 50rpx;
	line-height: 50rpx;
	width: 460rpx;
}
.phone_tag {
	width: 380rpx;
	font-size: 28rpx;
	font-family: PingFang SC;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
}
.phone_wrap image {
	width: 34rpx;
	height: 28rpx;
	margin-left: 30rpx;
	margin-top: 10rpx;
}

